{
  "name": "rwebsocket",
  "version": "1.0.1",
  "description": "Simple auto-reconnect WebSocket decorator",
  "main": "rwebsocket.js",
  "scripts": {
    "test": "npm run lint && mocha",
    "lint": "eslint rwebsocket.js test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxleiko/rwebsocket.git"
  },
  "keywords": [
    "websocket",
    "ws",
    "reconnect",
    "auto",
    "retry"
  ],
  "author": "Maxime Tricoire <max.tricoire@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maxleiko/rwebsocket/issues"
  },
  "homepage": "https://github.com/maxleiko/rwebsocket#readme",
  "devDependencies": {
    "eslint": "^3.12.2",
    "mocha": "^3.2.0"
  },
  "dependencies": {
    "ws": "^1.1.1"
  },
  "optionalDependencies": {
    "bufferutil": "^1.3.0",
    "utf-8-validate": "^2.0.0"
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "rules": {
      "semi": 2,
      "no-use-before-define": 2,
      "space-before-function-paren": ["error", {
        "anonymous": "always",
        "named": "never",
        "asyncArrow": "ignore"
      }]
    }
  }
}
